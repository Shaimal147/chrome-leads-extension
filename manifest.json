{
  "manifest_version": 3,
  "name": "Leads tracker",
  "version": "1.0",
  "description": "A simple extension to save leads url",

  "action": {
    "default_popup": "./index.html",
    "default_title": "Leads tracker",
    "default_icon": "./icons8-dollar-96.png"
  },

  "permissions": [
    "storage",
    "tabs"
  ],

  "host_permissions": [
    "<all_urls>"
  ]
}
